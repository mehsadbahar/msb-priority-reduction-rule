    "def leftmost_first_reduction(vector):\n",
    "    \"\"\"Ø³ÛŒØ§Ø³Øª Ú©Ø§Ù‡Ø´ Ø§Ø² Ú†Ù¾â€ŒØªØ±ÛŒÙ† Ù…ÙˆÙ‚Ø¹ÛŒØª ØºÛŒØ±ØµÙØ±\"\"\"\n",
    "    vec = list(vector)\n",
    "    states = [tuple(vec)]\n",
    "    \n",
    "    while any(x > 0 for x in vec):\n",
    "        for i in range(len(vec)):\n",
    "            if vec[i] > 0:\n",
    "                vec[i] -= 1\n",
    "                states.append(tuple(vec))\n",
    "                break\n",
    "    \n",
    "    return states\n",
    "\n",
    "def greedy_largest_reduction(vector):\n",
    "    \"\"\"Ø³ÛŒØ§Ø³Øª Ú©Ø§Ù‡Ø´ Ø§Ø² Ø¨Ø²Ø±Ú¯ØªØ±ÛŒÙ† Ù…Ù‚Ø¯Ø§Ø±\"\"\"\n",
    "    vec = list(vector)\n",
    "    states = [tuple(vec)]\n",
    "    \n",
    "    while any(x > 0 for x in vec):\n",
    "        max_val = max(vec)\n",
    "        max_index = vec.index(max_val)\n",
    "        vec[max_index] -= 1\n",
    "        states.append(tuple(vec))\n",
    "    \n",
    "    return states"
   ]
